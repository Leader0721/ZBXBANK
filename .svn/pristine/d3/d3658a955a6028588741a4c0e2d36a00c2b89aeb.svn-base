    outline: none;
}

.left_name {
    font-family: 'Helvetica Neue', Helvetica, sans-serif;
    line-height: 1.1;
    float: left;
    width: 35%;
    padding: 10px 10px
}

.input_normal .right_textArea {
    font-family: 'Helvetica Neue', Helvetica, sans-serif;
    font-size: 1rem;
    width: 65%;
    height: 3rem;
    border: 0;
    background: #efeff4;
    border-radius: 5px;
    padding: 1%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.inputEmptyBorder {
    border: #f07947 1px solid !important;
    -webkit-animation-name: fadeOut; /*动画名称*/
    -webkit-animation-duration: 0.2s; /*动画持续时间*/
    -webkit-animation-iteration-count: 1; /*动画次数*/
    -webkit-animation-delay: 0s; /*延迟时间*/
}

.redStar {
    color: red;
    font-size: 2rem;
    float: left;
    line-height: 2.8rem;
    width: 1rem;
}

.history {
    margin-left: 5%;
