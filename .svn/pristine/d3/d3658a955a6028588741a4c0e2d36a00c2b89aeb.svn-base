/*普通输入框div样式*/
.input_normal {
    position: relative;
    clear: left;
    overflow: hidden;
    margin-right: 10px;
    margin-top: 10px;
}

.input_normal label {
    font-family: 'Helvetica Neue', Helvetica, sans-serif;
    font-size: 1rem;
    line-height: 1.1;
    float: left;
    width: 35%;
    padding: 10px;
    color: #8f8f94;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.input_normal .right_input {
    font-family: 'Helvetica Neue', Helvetica, sans-serif;
    font-size: 1rem;
    background: #efeff4;
    border-radius: 5px;
    float: right;
    width: 65%;
    margin-bottom: 0;
    padding-left: 10px;
    padding-right: 10px;
    border: #efeff4 1px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    height: 40px;
    outline: none;
}

.input_normal .right_input_date {
    float: left;
    height: 2rem;
    width: 50%;
    font-size: 0.9rem;
    line-height: 2rem;
}

