<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/html">

<head>
    <meta charset="utf-8">
    <title>安全性认定-第二还款来源</title>
    <meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">

    <!--标准mui.css-->
    <link rel="stylesheet" href="css/mui.min.css">

    <link rel="stylesheet" type="text/css" href="css/style.css"/>
    <link rel="stylesheet" type="text/css" href="css/popModal.css"/>
    <script src="js/jquery-3.1.1.js" type="text/javascript" charset="utf-8"></script>
    <script src="js/formText.js" type="text/javascript"></script>
    <script src="js/popModal.js" type="text/javascript"></script>

    <script type="text/javascript">
        function getJsonData(){
            var jsonData = getJsonByForm();

            return jsonData;
        }

        function saveData(submitType) {
            if (!checkRegOfBank()) {
                submitObject(getJsonData(), "ChinaRCB/AddInvestigateThree", 0, 1);
            }else{
                alertMsg("请修改不符合规则的内容");
            }
        }

        var observerPoint;
        function watchValue() {
            var gsDate = $("#ReportDateFirst").val();
            var gsDate2 = $("#ReportDateSecond").val();
            if (gsDate != "" && gsDate2 != "") {
                $("#reportDate1").html(gsDate);
                $("#reportDate2").html(gsDate2);
                clearInterval(observerPoint);
            }
        }

        $(function(){
            initFormFocus();
            initSubmitStatus();
            initBaseInfo();

            observerPoint = setInterval(watchValue, 1000);
        });
    </script>
</head>
<body>
<header class="mui-bar mui-bar mui-bar-nav">
    <a class="mui-icon mui-pull-left" style="color: white;margin-left:4px;" href="javascript:goBack();"><img src="img/toolbar_back_white.png" width="24" height="24"></a>
    <h1 class="mui-title">安全性认定-第二还款来源</h1>
    <a id="saveStatus" class="mui-icon mui-btn-blue mui-btn-link mui-pull-right"
       style="color: white; margin-right: 10px; margin-top:-7px"
       href="javascript:saveData();"><img src="img/save.png" width="20" height="20"></a>
</header>
<form name="form">
    <article class="content">
        <section>
            <div class="information" style="margin-top: 0;" id="GuaranteeFirst">
                <p>保证人一调查</p>
            </div>
            <div style="height: 1rem;"></div>
            <ul class="header_inf">
                <li>内容</li>
                <li><input type="text" id="ReportDateFirst" readonly="readonly"/></li>
                <li>较上年同期</li>
            </ul>
            <div class="content_text">
                <span>总资产</span>
                <input id="GeneralAssetsFirst" type="number" step="0.01">
                <input id="GeneralAssets2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>固定资产</span>
                <input id="FixedAssetsFirst" type="number" step="0.01">
                <input id="FixedAssets2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>净资产</span>
                <input id="OwnersEquityFirst" type="number" step="0.01">
                <input id="OwnersEquity2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>资产负债率</span>
                <div class="ratio"><input id="AssetLiiabilityRatioFirst" type="number"><label>%</label></div>
                <div class="ratio" style="border-right: 0"><input id="AssetLiiabilityRatio2First" type="number"><label>%</label></div>
            </div>
            <div class="content_text">
                <span>流动比率</span>
                <div class="ratio"><input id="CurrentRatioFirst" type="number"><label>%</label></div>
                <div class="ratio" style="border-right: 0"><input id="CurrentRatio2First" type="number"><label>%</label></div>
            </div>
            <div class="content_text">
                <span>存货周转率</span>
                <input id="StockTurnoverRateFirst" type="number" step="0.01">
                <input id="StockTurnoverRate2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>应收账款周转率</span>
                <input id="ReceivableTurnoverRateFirst" type="number" step="0.01">
                <input id="ReceivableTurnoverRate2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>总负债</span>
                <input id="GrossLiabilitiesFirst" type="number" step="0.01">
                <input id="GrossLiabilities2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>流动负债</span>
                <input id="CashLiabilitiesFirst" type="number" step="0.01">
                <input id="CashLiabilities2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>长期负债</span>
                <input id="LongLiabilitiesFirst" type="number" step="0.01">
                <input id="LongLiabilities2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>在信用社贷款额</span>
                <input id="OurLoanFirst" type="number" step="0.01">
                <input id="OurLoan2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>不良贷款额</span>
                <input id="BadLoanFirst" type="number" step="0.01">
                <input id="BadLoan2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>销售收入</span>
                <input id="MainTakeInFirst" type="number" step="0.01">
                <input id="MainTakeIn2First" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>利润总额</span>
                <input id="TotalInComeFirst" type="number" step="0.01">
                <input id="TotalInCome2First" type="number" step="0.01">
            </div>


            <div class="input_normal">
                <label>本次承担保证金额</label><input class="right_input" type="number" step="0.01"
                                                              id="CurrentGuanateAmountFirst" />
            </div>
            <div class="input_normal">
                <label>已为他人担保</label><input class="right_input" type="number" step="0.01"
                                                            id="HaveGuanateAmountFirst" />
            </div>
            <div class="input_normal">
                <label>已抵押资产额</label><input class="right_input" type="number" step="0.01"
                                                            id="MortgagedAssetsFirst" />
            </div>
            <div class="input_normal">
                <label>保证人报告期（<span id="reportDate1"></span>份）信用等级</label><input class="right_input" type="text"
                                                                    id="ReportDateCreditRatingFirst" />
            </div>

            <div class="input_normal">
                <label>保证人年度信用等级</label><input class="right_input" type="text"
                                                               id="AnnualCreditRatingFirst" />
            </div>
            <div class="input_normal">
                <label>保证人其他认定</label><input class="right_input" type="text"
                                                             id="CompanyNameFirst" />
            </div>
        </section>

        <section>
            <div class="information" id="GuaranteeSecond">
                <p>保证人二调查</p>
            </div>
            <div style="height: 1rem;"></div>
            <ul class="header_inf">
                <li>内容</li>
                <li><input type="text" id="ReportDateSecond" readonly="readonly"/></li>
                <li>较上年同期</li>
            </ul>
            <div class="content_text">
                <span>总资产</span>
                <input id="GeneralAssetsSecond" type="number" step="0.01">
                <input id="GeneralAssets2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>固定资产</span>
                <input id="FixedAssetsSecond" type="number" step="0.01">
                <input id="FixedAssets2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>净资产</span>
                <input id="OwnersEquitySecond" type="number" step="0.01">
                <input id="OwnersEquity2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>资产负债率</span>
                <div class="ratio"><input id="AssetLiiabilityRatioSecond" type="number"><label>%</label></div>
                <div class="ratio" style="border-right: 0"><input id="AssetLiiabilityRatio2Second" type="number"><label>%</label></div>
            </div>
            <div class="content_text">
                <span>流动比率</span>
                <div class="ratio"><input id="CurrentRatioSecond" type="number"><label>%</label></div>
                <div class="ratio" style="border-right: 0"><input id="CurrentRatio2Second" type="number"><label>%</label></div>
            </div>
            <div class="content_text">
                <span>存货周转率</span>
                <input id="StockTurnoverRateSecond" type="number" step="0.01">
                <input id="StockTurnoverRate2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>应收账款周转率</span>
                <input id="ReceivableTurnoverRateSecond" type="number" step="0.01">
                <input id="ReceivableTurnoverRate2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>总负债</span>
                <input id="GrossLiabilitiesSecond" type="number" step="0.01">
                <input id="GrossLiabilities2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>流动负债</span>
                <input id="CashLiabilitiesSecond" type="number" step="0.01">
                <input id="CashLiabilities2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>长期负债</span>
                <input id="LongLiabilitiesSecond" type="number" step="0.01">
                <input id="LongLiabilities2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>在信用社贷款额</span>
                <input id="OurLoanSecond" type="number" step="0.01">
                <input id="OurLoan2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>不良贷款额</span>
                <input id="BadLoanSecond" type="number" step="0.01">
                <input id="BadLoan2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>销售收入</span>
                <input id="MainTakeInSecond" type="number" step="0.01">
                <input id="MainTakeIn2Second" type="number" step="0.01">
            </div>
            <div class="content_text">
                <span>利润总额</span>
                <input id="TotalInComeSecond" type="number" step="0.01">
                <input id="TotalInCome2Second" type="number" step="0.01">
            </div>


            <div class="input_normal">
                <label>本次承担保证金额</label><input class="right_input" type="number" step="0.01"
                                                              id="CurrentGuanateAmountSecond" />
            </div>
            <div class="input_normal">
                <label>已为他人担保</label><input class="right_input" type="number" step="0.01"
                                                            id="HaveGuanateAmountSecond" />
            </div>
            <div class="input_normal">
                <label>已抵押资产额</label><input class="right_input" type="number" step="0.01"
                                                            id="MortgagedAssetsSecond" />
            </div>
            <div class="input_normal">
                <label>保证人报告期（<span id="reportDate2"></span>份）信用等级</label><input class="right_input" type="text"
                                                                    id="ReportDateCreditRatingSecond" />
            </div>

            <div class="input_normal">
                <label>保证人年度信用等级</label><input class="right_input" type="text"
                                                               id="AnnualCreditRatingSecond" />
            </div>
            <div class="input_normal">
                <label>保证人其他认定</label><input class="right_input" type="text"
                                                             id="CompanyNameSecond" />
            </div>
        </section>

        <div class="location">
            <div class="basic">
                <div><a href="javascript:scrollToPlace('#GuaranteeFirst')">保证人一</a></div>
            </div>
            <div class="basic survey">
                <div><a href="javascript:scrollToPlace('#GuaranteeSecond')">保证人二</a></div>
            </div>
            <a id="callPhone"><img class="basic phone" src="img/phone.png"/></a>
        </div>
    </article>
</form>
<script type="text/javascript">
    $(window).scroll(function () {
        positionByScroll();
        //				显示div
        $(".location").show(300);
        //				存储当前滚动高度
        var Page = window.pageYOffset;
        setTimeout(function () {
            //					判断当前高度是否和存储的高度一致
            if (Page == window.pageYOffset) {
                //						隐藏div
                $(".location").hide();
            }
        }, 4000);
    });
    initPositionById("GuaranteeFirst","GuaranteeSecond");
</script>
</body>

</html>