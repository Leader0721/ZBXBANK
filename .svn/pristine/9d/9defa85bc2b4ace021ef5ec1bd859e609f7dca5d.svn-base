.headerNav{
    background-color: #454D57;
    align-items: center;
    justify-content: space-between;
}
.navTitle{
    font-size: 36;
    color: white;
    font-weight: bold;
    align-items: center;
    line-height: 88;
}

.left-image {
    position: absolute;
    bottom: 20;
    left: 28;
    width: 50;
    height: 50;
}
.right-image {
    position: absolute;
    bottom: 20;
    right: 28;
    width: 50;
    height: 50;
}

.baseInfoTitle {
    height: 60;
    line-height: 60;
    background-color: #f1f4f9;
    border-left-width: 4px;
    border-color: #31A4E5;
    padding-left: 20;
}
.titleText{
    font-size: 24;
    color: #31A4E5;
    line-height: 60;
}
.itemInfo{
    margin-left: 10;
    height: 80;
    line-height: 80;
    flex-direction: row;
    flex: 2;
    padding-top: 10;
}
.leftNormal{
    float: left;
    height: 60;
    line-height: 60;
    width: 300;
    justify-content: space-between;
}
.titleNormal{
    font-size: 24;
    height: 60;
    line-height: 60;
    float: left;
}
.rightNormal{
    height: 60;
    line-height: 60;
    align-items:center;
}

.inputNormal{
    font-size: 60;
    height: 50;
    width: 400;
    border-style: solid;
    border-color: #efeff4;
    border-width: 2;
    font-size: 24;
    background-color: #efeff4;
    border-radius: 5px;
    padding-left: 10;
    padding-right: 10;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}


.panel {
    width: 600px;
    height: 60px;
    margin-left: 75px;
    margin-top: 35px;
    margin-bottom: 35px;
    flex-direction: column;
    justify-content: center;
    border-width: 2px;
    border-style: solid;
    border-color: #DDDDDD;
    background-color: #F5F5F5;
}
.text {
    font-size: 50px;
    text-align: center;
    color: #41B883;
}

.cell{
    height: 200px;
    width: 450px;
}